<UserControl x:Class="MinecraftCrafterList.CustomUserControl.UIBackground"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MinecraftCrafterList.CustomUserControl"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Border x:Name="LeftTop" Background="#FFC6C6C6" BorderBrush="#FFDBDBDB" BorderThickness="2,2,0,0" CornerRadius="8,8,8,8"/>
        <Border x:Name="RightBottom" BorderBrush="#FF5B5B5B" BorderThickness="0,0,2,2" CornerRadius="8,8,8,8"/>
    </Grid>
</UserControl>
