<UserControl x:Class="MinecraftCrafterList.CustomUserControl.ItemSlot"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MinecraftCrafterList.CustomUserControl"
             mc:Ignorable="d" 
             d:DesignHeight="49" d:DesignWidth="49">
    <Grid x:Name="ItemSlotBackground" Background="#FF8B8B8B">
        <Border x:Name="ItemSlotLeftTop" BorderBrush="#FF373737" BorderThickness="2,2,0,0" Margin="0,0,2,2"/>
        <Border x:Name="ItemSlotRightBottom" BorderBrush="White" BorderThickness="0,0,2,2" Margin="2,2,0,0"/>
        <!--<Grid x:Name="ItemPlace" Margin="2"/>-->
    </Grid>
</UserControl>
